	<Style x:Key="RadSchedulerWindowStyle" BasedOn="{StaticResource RadWindowStyle}" TargetType="telerikNavigation:RadWindow" />
	
	<!-- DurationPicker -->
	<Style x:Key="RadDurationPickerStyle" BasedOn="{StaticResource RadComboBoxStyle}" TargetType="telerikInput:RadComboBox" />
	
	<Style TargetType="telerik:SchedulerWindow" BasedOn="{StaticResource RadSchedulerWindowStyle}" />
	<Style TargetType="telerik:DurationPicker" BasedOn="{StaticResource RadDurationPickerStyle}" />

</ResourceDictionary>